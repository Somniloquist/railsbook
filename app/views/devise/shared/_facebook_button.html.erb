    <%- if devise_mapping.omniauthable? %>
      <%- User.omniauth_providers.each do |provider| %>
        <%= link_to "#{yield(:facebook_btn_text)} #{OmniAuth::Utils.camelize(provider)}", omniauth_authorize_path(User, provider), method: :post, class: "btn btn-facebook w-100" %><br />
      <% end %>
    <% end %>
