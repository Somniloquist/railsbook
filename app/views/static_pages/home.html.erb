<div class="jumbotron">
  <div class="container welcome">
    <div class="row">
      <div class="col-md">

        <div id="call-to-action">
          <h1>Connect with people around the world</h1>
          <ul>
            <li>See photos and updates</li>
            <li>Share what's new</li>
            <li>Discover new connections</li>
          </ul>
        </div>
      </div>

      <div class="form-container col-md">
        <%= render "devise/registrations/form" %>
      </div>
    </div>
  </div>
</div>